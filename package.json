		"prettier": "^2.5.1",
		"react-test-renderer": "18.1.0",
		"tailwindcss": "^3.0.23",
		"typescript": "^4.3.5"
	},
	"jest": {
		"preset": "react-native"
	},
	"lint-staged": {
		"src/**/*.{js,ts,tsx}": [
			"./node_modules/.bin/eslint --ignore-path .gitignore",
			"prettier --write"
		]
	}
}
