{
	"name": "TessMobile",
	"version": "0.0.34",
	"private": true,
	"scripts": {
		"postinstall": "npm run patch && npm run build:tw && npm run init:styles && npm run init:fonts",
		"init": "npm run clean && npm install && npm run patch && npm run init:ios && npm run build:tw && npm run init:styles && npm run init:fonts",
		"init:win": "npm run clean:win && npm run patch && npm run build:tw && npm run init:styles && npm run init:fonts",
		"init:styles": "cd ext && npm link && vmrn ../src/styles/config/StyleMap.js -o ../src/styles/config/StyleKeys.ts ",
		"init:fonts": "npx react-native-asset",
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"ipad": "react-native run-ios --simulator='iPad Air (4th generation)'",
		"start": "react-native start",
		"start:dev": "npm run build:tw && npm run init:styles && npm run start",
		"test": "jest",
		"lint": "eslint src/**/*.{js, ts, tsx}",
		"init:ios": "cd ios && pod install",
		"build:tw": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
		"clean": "watchman watch-del-all ",
		"clean:start": "npm run clean && npm run build:tw && npm run init:styles && react-native start --reset-cache",
		"clean:win": "npm run build:tw && npm run init:styles && react-native start --reset-cache",
		"prepare": "husky install",
		"vmlint": "./node_modules/.bin/eslint --ignore-path .gitignore ./src/**/*.{js,ts,tsx}",
		"patch": "rimraf node_modules/.cache && npx @vm-lib/patch-node-modules -s node_modules_patches -o node_modules",
		"generate:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
		"debug:build": "npm run generate:bundle && cd android && ./gradlew assembleDebug",
		"release:build": "cd android && ./gradlew assembleRelease",
		"clean:android": "cd android && ./gradlew clean"
	},
	"dependencies": {
		"@howljs/calendar-kit": "^1.13.2",
